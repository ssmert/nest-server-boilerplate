{
    "name": "nest-server-boilerplate",
    "version": "0.0.1",
    "description": "NestJS Boilerplate, Typescript, Mysql, TypeORM",
    "author": "JongHyeon Jeon",
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
        "watch:dev": "nodemon --config nodemon.json",
        "start:debug": "nest start --debug --watch",
        "webpack": "webpack --config webpack.config.js --progress",
        "start:prod": "node dist/main.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint --fix . --ext .ts"
    },
    "dependencies": {
        "@nestjs/common": "^7.0.7",
        "@nestjs/core": "^7.0.7",
        "@nestjs/jwt": "^7.0.0",
        "@nestjs/microservices": "^7.0.7",
        "@nestjs/passport": "^7.0.0",
        "@nestjs/platform-express": "^7.0.7",
        "@nestjs/swagger": "^4.5.1",
        "@nestjs/typeorm": "^7.0.0",
        "bcrypt": "^5.0.0",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.1",
        "compression": "^1.7.4",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.1.1",
        "file-type": "^14.1.4",
        "helmet": "^3.22.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "mime-types": "^2.1.26",
        "morgan": "^1.10.0",
        "mysql2": "^2.1.0",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "reflect-metadata": "^0.1.13",
        "request-context": "^2.0.0",
        "rxjs": "^6.5.5",
        "source-map-support": "^0.5.16",
        "swagger-ui-express": "^4.1.4",
        "typeorm": "^0.2.24",
        "typeorm-transactional-cls-hooked": "^0.1.12",
        "typescript": "^3.8.3"
    },
    "devDependencies": {
        "@nestjs/testing": "^6.8.3",
        "@types/bcrypt": "^3.0.0",
        "@types/compression": "^1.0.1",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/express-rate-limit": "^3.3.3",
        "@types/file-type": "^10.9.1",
        "@types/helmet": "^0.0.44",
        "@types/jsonwebtoken": "^8.3.4",
        "@types/lodash": "^4.14.144",
        "@types/mime-types": "^2.1.0",
        "@types/morgan": "^1.7.37",
        "@types/node": "^12.7.12",
        "@types/passport-jwt": "^3.0.2",
        "@types/supertest": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "^2.5.0",
        "@typescript-eslint/eslint-plugin-tslint": "^2.5.0",
        "@typescript-eslint/parser": "^2.5.0",
        "clean-webpack-plugin": "^3.0.0",
        "nodemon": "^1.19.3",
        "prettier": "^1.18.2",
        "supertest": "^4.0.2",
        "ts-loader": "^6.2.0",
        "ts-node": "^8.4.1",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^5.20.0",
        "tslint-config-prettier": "^1.18.0",
        "webpack": "^4.41.1",
        "webpack-cli": "^3.3.9",
        "webpack-node-externals": "^1.7.2"
    }
}
